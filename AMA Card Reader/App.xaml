<Application x:Class="AMA_Card_Reader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AMA_Card_Reader"
             xmlns:converter="clr-namespace:AMA_Card_Reader.Converter"
             StartupUri="Views/CardReaderView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:StringToVisibilityConverter x:Key="StringToVisibilityConverter"/>
            <converter:FilePathToFileNameConverter x:Key="FilePathToFileNameConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
